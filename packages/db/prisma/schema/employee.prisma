model Employee {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  fullName  String
  email     String @unique
  pwHash    String
  picture   File?  @relation(fields: [pictureId], references: [id])
  pictureId Int?

  preferredLanguage String

  @@index([fullName])
  @@index([email])
}

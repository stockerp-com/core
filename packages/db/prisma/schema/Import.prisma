model Import {
  id        Int      @id @default(autoincrement())
  createdAt DateTime @default(now())
  updatedAt DateTime @updatedAt

  status ImportStatus
  source ImportSource

  file File? @relation(fields: [fileId], references: [id])

  fileId Int?
}

enum ImportStatus {
  PENDING
  PROCESSING
  PROCESSED
  FAILED
}

enum ImportSource {
  PROM
  TORGSOFT
}
